        "# JSON Lines íŒŒì¼ ì½ê¸°\n",
        "import json\n",
        "import pandas as pd\n",
        "\n",
        "def read_jsonl(file_path):\n",
        "    data = []\n",
        "    with open(file_path, 'r', encoding='utf-8') as f:\n",
        "        for line in f:\n",
        "            data.append(json.loads(line))\n",
        "    return pd.DataFrame(data)\n",
        "\n",
        "# ì˜ˆì¸¡ íŒŒì¼ ë¶ˆëŸ¬ì˜¤ê¸°\n",
        "pred1 = read_jsonl(\"/root/home/output (26).csv\")\n",
        "pred2 = read_jsonl(\"/root/home/output (31).csv\")\n",
        "pred3 = read_jsonl(\"/root/home/output (32).csv\")\n",
        "pred4 = read_jsonl(\"/root/home/output (33).csv\")\n",
        "\n",
        "# ì˜ˆì¸¡ ê²°ê³¼ë¥¼ ë¦¬ìŠ¤íŠ¸ë¡œ ì €ì¥ (ì—¬ê¸°ì„œëŠ” 'answer' ì»¬ëŸ¼ì„ ì‚¬ìš©)\n",
        "predictions = [pred1['answer'], pred2['answer'], pred3['answer'], pred4['answer']]\n",
        "\n",
        "# ë°ì´í„°í”„ë ˆì„ìœ¼ë¡œ ë³€í™˜í•˜ì—¬ ê° rowë§ˆë‹¤ í•˜ë“œë³´íŒ…ì„ ì ìš©\n",
        "pred_df = pd.DataFrame(predictions).T\n",
        "\n",
        "# í•˜ë“œ ë³´íŒ…ì„ ì ìš©\n",
        "def hard_voting(row):\n",
        "    return row.value_counts().idxmax()\n",
        "\n",
        "final_predictions = pred_df.apply(hard_voting, axis=1)\n",
        "\n",
        "# ê²°ê³¼ë¥¼ DataFrameìœ¼ë¡œ ì €ì¥\n",
        "output = pd.DataFrame({'summary': final_predictions})\n",
        "\n",
